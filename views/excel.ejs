<div class="page-content p-5" id="content">

  <div class="table-responsive">
    <table class="table table-hover">
        <thead>
          <tr id="head">
            <th scope="col">EmployeeId</th>
            <th scope="col">Panel Member</th>
            <th scope="col">JobCode</th>
            <th scope="col">RM</th>
            <th scope="col">Readiness</th>
            <th scope="col">Java</th>
            <th scope="col">CPP</th>
            <!-- <th scope="col">ACCEPTANCE</th> -->
            <!-- <th scope="col">DECLINE</th> -->
            <th scope="col"></th>
            <th scope="col"></th>
            <!-- <th scope="col">Date</th> -->
            <!-- <th scope="col">Month</th> -->
            <!-- <th scope="col">Add</th> -->
            <!-- <th scope="col">Remove</th> -->
          </tr>
         </thead>
        <tbody>
        <% for(var i=0;i<details.length;i++){%>
          <tr>
            <th scope="row"><%= details[i].emp_id %></th>
            <td><%= details[i].emp_name %></td>
            <td><%= details[i].job_code %></td>
            <td><%= details[i].rm %></td>
            <td><%= details[i].ready %></td>
            <td><%= details[i].java %></td>
            <td><%= details[i].cpp %></td>
            <!-- <td><%= details[i].d_2022_04_18%></td> -->
            <!-- <td><%= details[i].Decline%></td> -->
            <td><a href="edit?emp_id=<%= details[i].emp_id %>" id="edit"><button class="btn btn-lg btn-primary" type="button">EDIT</button></a></td>
            <td><a href="delete?emp_id=<%= details[i].emp_id %>" id="delete"><button class="btn  btn-lg btn-primary" type="button">DELETE</button></a></td>
            <!-- <form action="/add_interview" method="POST"><input type="hidden" placeholder="Enter date" name="date" class="txtDate" value="2022-04-01"> -->
              <!-- <td><Input type="month"></Input></td> -->
            <!-- <td><button class="btn btn-dark" type="Submit" name="add"><i class="fa fa-plus-circle"></i></button></td> -->
            <!-- <td><button class="btn btn-dark" type="Submit" name="remove" ><i class="fa fa-minus-circle" style="background-color: black;"></i></button></td></form> -->
          <!-- </tr>  -->
        <% } %>
        </tbody>
      </table>
    </div>
    <div class="icon">
      <a href="add" style="color: black;"><i class="fa fa-plus-circle"></i></a>
    </div>

</div>
    
<script>

const hamburger = document.querySelector(".hamburger");
const navMenu = document.querySelector(".nav-menu");

hamburger.addEventListener("click", mobileMenu);

function mobileMenu() {
    hamburger.classList.toggle("active");
    navMenu.classList.toggle("active");
}

const navLink = document.querySelectorAll(".nav-link");

navLink.forEach(n => n.addEventListener("click", closeMenu));

function closeMenu() {
    hamburger.classList.remove("active");
    navMenu.classList.remove("active");}

    </script>

  </body>
